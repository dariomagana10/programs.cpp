//#2
#include <iostream>

using namespace std;

int main() {
  int num1, num2;
  cout<<"This program do the product, division and divison of two numbers"<<endl;
  cout<<"Type the first number" <<endl;
  cin>>num1;
  cout<<"Type the second number";
  cin>>num2;
  cout<<"The product of the two numbers is: " <<" "<<num1*num2<<endl;
  cout<<"The integer divison of the two numbers is "<<" "<<num1/num2<<endl;
  cout<<"The remainder of the division of the two numbers is : "<<" "<<num1%num2<<endl;
  return 0;
}
